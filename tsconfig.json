{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ES6",
    "outDir": "./dist",
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "alwaysStrict": true,
    "skipLibCheck": true,
    "watch": true
  }
}
